Page{
  width: 100%;
}
.swiper{
  width: 100%;
  height: 278rpx;
}
.swiper swiper-item{
  width: 100%;
}
.swiper swiper-item image{
  width: 100%;
}
.nav{
  display: flex;
  width: 100%;
  padding: 20rpx 0;
  /* height: 200rpx; */
  
}
.font{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 20%;
}
.iconfont{
  width: 100rpx;
  height: 100rpx; 
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  background-color: #F01313;
  border-radius: 50rpx;
}
.text{
  font-size: 26rpx;
}
.content{
  width: 96%;
  margin: 0 auto;
  /* height: 50rpx; */
}
.scroll{
  display: flex;
  width: 100%;
  height: 300rpx;
}
.scroll navigator{
  margin: 10rpx;
}
.scroll image{
  width: 200rpx;
  border-radius: 20rpx;
}
.name{
  width: 200rpx;
  font-size: 25rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.toplist{
  height: 500rpx;
}
.a{
  width: 95%;
  margin: 0 auto;
}
.s{
  border-radius: 10rpx;
  background-color: #fafafa;

}
.topcontent{
  display: flex;
  align-items: center;
  flex-direction: row;
  padding-left: 13rpx;
  padding: 10rpx;
  font-size: 28rpx;
}
.tophead{
  /* width: 100rpx; */
  height: 50rpx;
}
.topleft image{
  width: 100rpx;
  border-radius: 8rpx;
  /* padding: 6rpx; */
}
.ss{
  margin-left: 30rpx;
}
.topright{
  margin-left: 30rpx;
  margin-right: 15rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.f{
  margin-left: 15rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}