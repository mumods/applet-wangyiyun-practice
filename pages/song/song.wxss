.head{
  width: 100%;
  position: relative;
}
.head image{
  width: 100%;
}
.date{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateY(-50%);
  margin-left: -100rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  width: 200rpx;
  height: 100rpx;
  /* border: 2rpx solid #777; */
}


.content{
  padding: 0rpx 20rpx;
}
.title{
  display: flex;
  padding: 20rpx 0rpx;
  justify-content: space-between;
}
.scroll{
  height: calc(100vh - 360rpx);
}
.song{
  display: flex;
  padding: 10rpx 0;
}
.img{
  width: 100rpx;
  height: 100rpx;
  border-radius: 5rpx;
}
.name{
  margin-left: 20rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.iconfont{
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  right: 0;
}